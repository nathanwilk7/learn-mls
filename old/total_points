<!-- MATCHES -->
<script id="ismt-fixtures-matches" type="text/template">

    <ul class="ism-multi-fixtures">

        <% _.each(teamsByMatches, function(data) { %>
            <li class="ism-multi-fixtures__item<% if (data.matches === 0) { %> ism-multi-fixtures__item--no-matches<% } %>">
                <h3 class="ism-multi-fixtures__heading">
                    <% if (data.matches === 0) { %>
                        Teams not playing this round
                    <% } else { %>
                        Teams playing <strong class="ism-multi-fixtures__number"><%= data.matches %></strong> matches this round:
                    <% } %>
                </h3>
                <ul class="ism-multi-fixtures__team-list">
                    <% _.each(data.teams, function(team, index) { %>
                        <% if (data.matches === 0) { %>
                            <li class="ism-multi-fixtures__team">
                        <% } else { %>
                            <li class="ism-multi-fixtures__team ism-multi-fixtures__team--multiple">
                                <img
                                    srcset="https://ismdj.scdn5.secure.raxcdn.com/static/libsass/mlsf/dist/img/badges/<%= team.get("id") %>.png 75w,
                                            https://ismdj.scdn5.secure.raxcdn.com/static/libsass/mlsf/dist/img/badges/<%= team.get("id") %>-large.png 150w"
                                    sizes="40px"
