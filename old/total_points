<!-- STANDINGS H2H MATCH -->
<script id="ismt-leagues-standings-h2h-match" type="text/template">
    <% if (showRound) {%>
        <td><%- is_knockout ? matchTitle : event %></td>
    <% } %>
    <td class="ism-col-entry-1">
        <% if (entry_1_entry) { %>
            <a href="<%= url('team_history', {'substitutions': {':entryId': entry_1_entry, ':eventId': matchLinkEvent}}) %>" class="ismjs-link ism-table__entry-link ism-table--matches__entry-link-1">
                <span class="ism-table--matches__entry-item-1"><strong><%- entry_1_name %></strong></span>
                <span class="ism-table--matches__entry-item-1"><%- entry_1_player_name %></span>
            </a>
        <% } else { %>
                <span class="ism-table--matches__entry-item-1"><%- entry_1_name %></span>
        <% } %>
    </td>
    <td class="ism-col-score">
        <% if (hasStarted) { %>
            <%= entry_1_points %> - <%= entry_2_points %>
        <% } else { %>
            v
        <% } %>
    </td>
    <td class="ism-col-entry-2">
        <% if (entry_2_entry) { %>
            <a href="<%= url('team_history', {'substitutions': {':entryId': entry_2_entry, ':eventId': matchLinkEvent}}) %>" class="ismjs-link ism-table__entry-link">
                <span class="ism-table--matches__entry-item-2"><strong><%- entry_2_name %></strong></span>
                <span class="ism-table--matches__entry-item-2"><%- entry_2_player_name %></span>
            </a>
        <% } else {%>
                <span class="ism-table--matches__entry-item-2"><%- entry_2_name %></span>
        <% } %>
    </td>
</script>

<!-- STANDINGS H2H MATCH - TIEBREAK CASE -->
