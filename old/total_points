                            <div class="ism-scoreboard-points__copy ism-scoreboard-points__panel__value"><%=  _.str.numberFormat(entry_history.rank) || '-' %></div>
                        </div>
                    </div>
                    <div class="ism-scoreboard-points__panel-container">
                        <div class="ism-scoreboard-points__panel">
                            <h3 class="ism-scoreboard-points__copy ism-scoreboard-points__panel__heading">Transfers</h3>
                            <div class="ism-scoreboard-points__copy ism-scoreboard-points__panel__value">
                                <%= entry_history.event_transfers %>
                                <% if (entry_history.event_transfers_cost) { %>
                                    (-<%= entry_history.event_transfers_cost %>pts)
                                <% } %>
                            </div>
                        </div>
                    </div>

                </div>

            </div>

            <div class="ism-scoreboard-points__switch">
                <div class="ism-subs-switch">
                    <div class="ism-subs-switch-inner">
                        <label for="flip-checkbox-1" class="ism-subs-switch__label">Manager subs:</label>
                        <% if (subsNeedEnabling === "confirmed") { %>
                            <a href="#" class="ismjs-subs-confirmed ism-subs-switch__link">Active</a>
                        <% } else { %>
                            <input type="checkbox" name="flip-checkbox-1" id="flip-checkbox-1" class="ismjs-flipswitch__input" <%- subsNeedEnabling === "on" ? "checked" : "" %>>
                        <% } %>
                    </div>
                </div>
            </div>

        </div>

    </div>

    <div class="ism-button-bar">
        <div class="ism-button-bar__button ism-button-bar__button--single">
           <button type="button" <%= !changed ? 'disabled' : '' %> class="ismjs-save ism-button--primary ism-button--wide">Save your team
               <svg class="ism-icon ism-icon--small ism-button__icon">
                 <use xlink:href="#ism-triangle"></use>
               </svg>
           </button>
        </div>
    </div>

</script>

